class CityExt {

    public string name = "";
    public double latitude = 0.0;
    public double longitude = 0.0;

    public CityExt(string name, double latitude, double longitude) {
        this.name = name;
        this.latitude = latitude;
        this.longitude = longitude;
    }
}
